    "core-js": "3.39.0",
    "crypto-js": "4.2.0",
    "date-fns": "4.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-recommended": "4.1.0",
    "eth-testing": "1.9.1",
    "ethers": "6.12.1",
