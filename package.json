    "jest-runner": "patch:jest-runner@npm:27.5.1#.yarn/patches/jest-runner-npm-27.5.1-2ed2c1cda8"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$(git rev-parse HEAD) PORT=3010 react-app-rewired start",
    "start-home": "yarn lingui:prepare && REACT_APP_VERSION=$(git rev-parse HEAD) PORT=3010 env-cmd -e development-home react-app-rewired start",
    "start-app": "yarn lingui:prepare && REACT_APP_VERSION=$(git rev-parse HEAD) PORT=3011 env-cmd -e development-app react-app-rewired start",
    "start-win": "set PORT=3010&react-app-rewired start",
    "build": "REACT_APP_VERSION=$(git rev-parse HEAD) INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "build-home": "REACT_APP_VERSION=$(git rev-parse HEAD) INLINE_RUNTIME_CHUNK=false env-cmd -e production-home react-app-rewired build",
    "build-app": "REACT_APP_VERSION=$(git rev-parse HEAD) INLINE_RUNTIME_CHUNK=false env-cmd -e production-app react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "react-app-rewired test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "node node_modules/.bin/eslint src",
    "check:ci": "yarn lint && yarn test:ci && yarn tscheck",
    "pre-commit": "npm run test -- --watchAll=false && npm run lint",
    "prepare": "husky install && yarn lingui:prepare",
    "extract": "NODE_ENV=development lingui extract --clean",
    "compile": "lingui compile",
    "lingui:prepare": "NODE_ENV=development yarn extract && yarn compile",
    "lingui:generate": "NODE_ENV=development lingui extract --overwrite",
    "tscheck": "tsc -p tsconfig.json --noEmit",
    "prettier": "prettier --write src/**/*.{js,ts,jsx,tsx,css,scss}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./jest-env.js"
    ],
    "globalSetup": "./jest-global-setup.js",
    "prettierPath": null
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@lingui/cli": "4.10.0",
    "@lingui/loader": "4.10.0",
    "@lingui/macro": "4.10.0",
    "@types/react-date-range": "1.4.9",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "buffer": "6.0.3",
    "eslint": "^8.41.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-local-rules": "2.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-perf": "3.3.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "react-app-rewired": "2.2.1",
    "tailwindcss": "3.4.1"
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx}": [
      "eslint --fix --max-warnings=0 ./src",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
