    "pre-commit": "npm run test -- --watchAll=false && npm run lint",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "**/*.js?(x)": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.1.0"
